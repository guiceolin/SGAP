.header.block
  %h1
    Editando Grupo da #{@group.enunciation.name}
.block
  = simple_form_for @group, url: professor_enunciation_group_path(@group.enunciation), html: { class: 'form-horizontal' } do |f|
    = render :partial => 'form', locals: { f:f }
    .form-actions
      = f.submit class: 'btn btn-primary'
