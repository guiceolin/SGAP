.block.header
  .title
    %h1 Importar CSV
  %br.clear

.block
  = form_tag admin_imports_path, method: :post, multipart: true, class: 'form-inline' do
    = select_tag 'resource', options_for_select([["Disciplinas", "subjects"],["Turmas", "crowds"],["Estudantes", "students"],["Professores",'professors']])
    = file_field_tag :csv_file
    = submit_tag "Importar"
